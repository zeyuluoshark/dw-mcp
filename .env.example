# export HOLOGRES_SOCIAL_MEDIA_DB="social_media"
# export HOLOGRES_SOCIAL_MEDIA_USER="BASIC\$mds"
# export HOLOGRES_SOCIAL_MEDIA_PASSWORD="your_password"

